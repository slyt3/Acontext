S3_ENDPOINT=http://127.0.0.1:19000
S3_REGION=auto
S3_ACCESS_KEY=your-dev-access-key
S3_SECRET_KEY=your-dev-secret-key
S3_BUCKET=acontext-dev
S3_LOCATION=./db/minio

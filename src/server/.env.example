# Ports
DATABASE_EXPORT_PORT=15432
REDIS_EXPORT_PORT=16379
RABBITMQ_EXPORT_PORT=15672
RABBITMQ_ADMIN_EXPORT_PORT=25672
S3_API_EXPORT_PORT=19000
S3_CONSOLE_EXPORT_PORT=19001
API_EXPORT_PORT=8029

# Root
ROOT_API_BEARER_TOKEN=your-root-api-bearer-token

# Postgres
DATABASE_NAME=acontext
DATABASE_USER=acontext
DATABASE_PASSWORD=helloworld
DATABASE_LOCATION=./db/pg

# Redis
REDIS_PASSWORD=helloworld
